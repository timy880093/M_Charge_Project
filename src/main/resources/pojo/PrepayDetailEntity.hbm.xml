<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.PrepayDetailEntity" table="prepay_detail" schema="public" catalog="charge_db">
        <id name="prepayDetailId">
            <column name="prepay_detail_id" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="prepayDeductMasterId">
            <column name="prepay_deduct_master_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="cashDetailId">
            <column name="cash_detail_id" sql-type="integer" />
        </property>
        <property name="companyId">
            <column name="company_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="calYm">
            <column name="cal_ym" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="money">
            <column name="money" sql-type="integer" not-null="true"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>