<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.BillCycleEntity" table="bill_cycle" schema="public" catalog="charge_db">
        <id name="billId">
            <column name="bill_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="billType">
            <column name="bill_type" sql-type="int4"/>
        </property>
        <property name="packageId">
            <column name="package_id" sql-type="int4"/>
        </property>
        <property name="companyId">
            <column name="company_id" sql-type="int4"/>
        </property>
        <property name="yearMonth">
            <column name="year_month" sql-type="varchar" length="6"/>
        </property>
        <property name="price">
            <column name="price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="cnt">
            <column name="cnt" sql-type="int4"/>
        </property>
        <property name="cntLimit">
            <column name="cnt_limit" sql-type="int4"/>
        </property>
        <property name="cntGift">
            <column name="cnt_gift" sql-type="int4"/>
        </property>
        <property name="cntOver">
            <column name="cnt_over" sql-type="int4"/>
        </property>
        <property name="priceOver">
            <column name="price_over" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="priceMax">
            <column name="price_max" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="payOver">
            <column name="pay_over" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="payMonth">
            <column name="pay_month" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="cashOutOverId">
            <column name="cash_out_over_id" sql-type="int4"/>
        </property>
        <property name="cashOutMonthId">
            <column name="cash_out_month_id" sql-type="int4"/>
        </property>
        <property name="cashInOverId">
            <column name="cash_in_over_id" sql-type="int4"/>
        </property>
        <property name="cashInMonthId">
            <column name="cash_in_month_id" sql-type="int4"/>
        </property>
        <property name="status">
            <column name="status" sql-type="bpchar" length="1"/>
        </property>
        <property name="isPriceFree">
            <column name="is_price_free" sql-type="varchar" length="1"/>
        </property>
        <property name="singlePrice">
            <column name="single_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
    </class>
</hibernate-mapping>