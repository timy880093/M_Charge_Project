<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


    <class name="dao.CommissionLogEntity" table="commission_log" schema="public" catalog="charge_db">
        <id name="commissionLogId">
            <column name="commission_log_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="commissionCpId">
            <column name="commission_cp_id" sql-type="int4"/>
        </property>
        <property name="commissionYmd">
            <column name="commission_ymd" sql-type="varchar" length="8"/>
        </property>
        <property name="userCpId">
            <column name="user_cp_id" sql-type="int4"/>
        </property>
        <property name="inDateStart">
            <column name="in_date_start" sql-type="date"/>
        </property>
        <property name="inDateEnd">
            <column name="in_date_end" sql-type="date"/>
        </property>
        <property name="commissionType">
            <column name="commission_type" sql-type="bpchar" length="1"/>
        </property>
        <property name="mainAmount">
            <column name="main_amount" sql-type="numeric" precision="53" scale="4"/>
        </property>
        <property name="mainPercent">
            <column name="main_percent" sql-type="numeric" precision="10" scale="5"/>
        </property>
        <property name="collectMoney">
            <column name="collect_money" sql-type="numeric" precision="53" scale="4"/>
        </property>
        <property name="addtionPercent">
            <column name="addtion_percent" sql-type="numeric" precision="10" scale="5"/>
        </property>
        <property name="inAmount">
            <column name="in_amount" sql-type="numeric" precision="53" scale="4"/>
        </property>
        <property name="commissionAmount">
            <column name="commission_amount" sql-type="numeric" precision="53" scale="4"/>
        </property>
        <property name="isPaid">
            <column name="is_paid" sql-type="bpchar" length="1"/>
        </property>
        <property name="note">
            <column name="note" sql-type="varchar" length="200"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
    </class>
</hibernate-mapping>