<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.UserEntity" table="user" schema="public" catalog="charge_db">
        <id name="userId">
            <column name="user_id" sql-type="serial" length="10" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="companyId">
            <column name="company_id" sql-type="int4" length="10"/>
        </property>
        <property name="roleId">
            <column name="role_id" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="account">
            <column name="account" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="30"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="50"/>
        </property>
        <property name="close">
            <column name="close" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4" length="10"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp" length="29" precision="6"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4" length="10"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp" length="29" precision="6"/>
        </property>
        <property name="printerId">
            <column name="printer_id" sql-type="int4" length="10"/>
        </property>
        <property name="authKey">
            <column name="auth_key" sql-type="varchar" length="150"/>
        </property>
        <property name="authUrl">
            <column name="auth_url" sql-type="varchar" length="50"/>
        </property>
        <property name="logoutTime">
            <column name="logout_time" sql-type="int4" length="10"/>
        </property>
    </class>
</hibernate-mapping>