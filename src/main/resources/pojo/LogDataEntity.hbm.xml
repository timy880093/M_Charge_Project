<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.LogDataEntity" table="log_data" schema="public" catalog="charge_db">
        <id name="logId">
            <column name="log_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="logUrl">
            <column name="log_url" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="parameterId">
            <column name="parameter_id" sql-type="int4"/>
        </property>
        <property name="accessTime">
            <column name="access_time" sql-type="timestamp"/>
        </property>
        <property name="userName">
            <column name="user_name" sql-type="varchar" length="30"/>
        </property>
        <property name="requestObj">
            <column name="request_obj" sql-type="text" length="2147483647"/>
        </property>
        <property name="sessionObj">
            <column name="session_obj" sql-type="text" length="2147483647"/>
        </property>
    </class>
</hibernate-mapping>