<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context 
	http://www.springframework.org/schema/context/spring-context-3.0.xsd
    ">
    
	<bean id="aes" class="com.meshinnovation.base.encrypt.aes.AES">
		<constructor-arg>
			<bean class="com.meshinnovation.base.encrypt.aes.AESSecretKey">
				<!-- -64,-68,-92,33,-57,-70,76,-122,-40,-10,98,-73,-23,73,11,-65 -->
				<constructor-arg>
					<list>
						<value>-64</value>
						<value>-68</value>
						<value>-92</value>
						<value>33</value>
						<value>-57</value>
						<value>-70</value>
						<value>76</value>
						<value>-122</value>
						<value>-40</value>
						<value>-10</value>
						<value>98</value>
						<value>-73</value>
						<value>-23</value>
						<value>73</value>
						<value>11</value>
						<value>-65</value>
					</list>
				</constructor-arg>
			</bean>
		</constructor-arg>
	</bean>
</beans>
