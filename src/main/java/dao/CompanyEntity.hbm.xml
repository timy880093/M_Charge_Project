<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.CompanyEntity" table="company" schema="public" catalog="charge_db">
        <id name="companyId">
            <column name="company_id" sql-type="serial" length="10" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="parentId">
            <column name="parent_id" sql-type="int4" length="10"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="40" not-null="true"/>
        </property>
        <property name="businessNo">
            <column name="business_no" sql-type="bpchar" length="8" not-null="true"/>
        </property>
        <property name="companyType">
            <column name="company_type" sql-type="int4" length="10"/>
        </property>
        <property name="verifyStatus">
            <column name="verify_status" sql-type="int4" length="10"/>
        </property>
        <property name="codeName">
            <column name="code_name" sql-type="varchar" length="10"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="20"/>
        </property>
        <property name="fax">
            <column name="fax" sql-type="varchar" length="20"/>
        </property>
        <property name="contact1">
            <column name="contact1" sql-type="varchar" length="40"/>
        </property>
        <property name="contactPhone1">
            <column name="contact_phone1" sql-type="varchar" length="20"/>
        </property>
        <property name="contact2">
            <column name="contact2" sql-type="varchar" length="40"/>
        </property>
        <property name="contactPhone2">
            <column name="contact_phone2" sql-type="varchar" length="20"/>
        </property>
        <property name="cityId">
            <column name="city_id" sql-type="int4" length="10"/>
        </property>
        <property name="companyAddress">
            <column name="company_address" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="mailingAddress">
            <column name="mailing_address" sql-type="varchar" length="100"/>
        </property>
        <property name="taxNo">
            <column name="tax_no" sql-type="bpchar" length="9"/>
        </property>
        <property name="taxOffice">
            <column name="tax_office" sql-type="varchar" length="50"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4" length="10"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp" length="29" precision="6"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4" length="10"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp" length="29" precision="6"/>
        </property>
        <property name="transferType">
            <column name="transfer_type" sql-type="int4" length="10"/>
        </property>
        <property name="topBanner">
            <column name="top_banner" sql-type="varchar" length="40"/>
        </property>
        <property name="bottomBanner">
            <column name="bottom_banner" sql-type="varchar" length="40"/>
        </property>
        <property name="email1">
            <column name="email_1" sql-type="varchar" length="100"/>
        </property>
        <property name="email2">
            <column name="email_2" sql-type="varchar" length="100"/>
        </property>
        <property name="companyKey">
            <column name="company_key" sql-type="varchar" length="50"/>
        </property>
    </class>
</hibernate-mapping>