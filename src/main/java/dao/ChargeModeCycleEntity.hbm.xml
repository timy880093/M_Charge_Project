<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.ChargeModeCycleEntity" table="charge_mode_cycle" schema="public" catalog="charge_db">
        <id name="chargeId">
            <column name="charge_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="packageName">
            <column name="package_name" sql-type="varchar" length="50"/>
        </property>
        <property name="chargeCycle">
            <column name="charge_cycle" sql-type="int4"/>
        </property>
        <property name="baseQuantity">
            <column name="base_quantity" sql-type="int4"/>
        </property>
        <property name="singlePrice">
            <column name="single_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="maxPrice">
            <column name="max_price" sql-type="int4"/>
        </property>
        <property name="salesPrice">
            <column name="sales_price" sql-type="int4"/>
        </property>
        <property name="prePayment">
            <column name="pre_payment" sql-type="int4"/>
        </property>
        <property name="freeQuantity">
            <column name="free_quantity" sql-type="int4"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int4"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="freeMonth">
            <column name="free_month" sql-type="int4"/>
        </property>
        <property name="contractLimit">
            <column name="contract_limit" sql-type="int4"/>
        </property>
        <property name="prepaidAdditionEnable">
            <column name="prepaid_addition_enable" sql-type="int4"/>
        </property>
        <property name="feePeriod">
            <column name="fee_period" sql-type="int4"/>
        </property>
    </class>
</hibernate-mapping>