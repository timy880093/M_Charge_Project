<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.DealerCompanyEntity" table="dealer_company" schema="public" catalog="charge_db">
        <id name="dealerCompanyId">
            <column name="dealer_company_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="mainAmount">
            <column name="main_amount" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="mainPercent">
            <column name="main_percent" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="collectMoney">
            <column name="collect_money" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="commissionType">
            <column name="commission_type" sql-type="bpchar" length="1"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int4"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="additionPercent">
            <column name="addition_percent" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="dealerCompanyName">
            <column name="dealer_company_name" sql-type="varchar" length="100"/>
        </property>
        <property name="businessNo">
            <column name="business_no" sql-type="bpchar" length="8"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="50"/>
        </property>
        <property name="fax">
            <column name="fax" sql-type="varchar" length="50"/>
        </property>
        <property name="companyAddress">
            <column name="company_address" sql-type="varchar" length="100"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>