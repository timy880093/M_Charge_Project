<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.GiftEntity" table="gift" schema="public" catalog="charge_db">
        <id name="giftId">
            <column name="gift_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="companyId">
            <column name="company_id" sql-type="int4"/>
        </property>
        <property name="giftDate">
            <column name="gift_date" sql-type="date"/>
        </property>
        <property name="giftCnt">
            <column name="gift_cnt" sql-type="int4"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="calId">
            <column name="cal_id" sql-type="int4"/>
        </property>
        <property name="outId">
            <column name="out_id" sql-type="int4"/>
        </property>
        <property name="inId">
            <column name="in_id" sql-type="int4"/>
        </property>
    </class>
</hibernate-mapping>