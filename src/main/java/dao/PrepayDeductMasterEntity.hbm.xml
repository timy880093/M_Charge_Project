<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.PrepayDeductMasterEntity" table="prepay_deduct_master" schema="public" catalog="charge_db">
        <id name="prepayDeductMasterId">
            <column name="prepay_deduct_master_id" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="companyId">
            <column name="company_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="amount">
            <column name="amount" sql-type="integer" not-null="true"/>
        </property>
        <property name="isEnableOver">
            <column name="is_enable_over" sql-type="char" not-null="true"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>