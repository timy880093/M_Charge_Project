<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.DealerEntity" table="dealer" schema="public" catalog="charge_db">
        <id name="dealerId">
            <column name="dealer_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="dealerName">
            <column name="dealer_name" sql-type="varchar" length="100"/>
        </property>
        <property name="dealerPhone">
            <column name="dealer_phone" sql-type="varchar" length="50"/>
        </property>
        <property name="dealerEmail">
            <column name="dealer_email" sql-type="varchar" length="100"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="dealerCompanyId">
            <column name="dealer_company_id" sql-type="int4"/>
        </property>
    </class>
</hibernate-mapping>