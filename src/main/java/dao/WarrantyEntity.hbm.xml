<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.WarrantyEntity" table="warranty" schema="public" catalog="charge_db">
        <id name="warrantyId">
            <column name="warranty_id" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="warrantyNo">
            <column name="warranty_no" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="companyId">
            <column name="company_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="startDate">
            <column name="start_date" sql-type="date" not-null="true"/>
        </property>
        <property name="endDate">
            <column name="end_date" sql-type="date" not-null="true"/>
        </property>
        <property name="extend">
            <column name="extend" sql-type="boolean" not-null="true"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="note">
            <column name="note" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="integer" not-null="true"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp(6)" not-null="true"/>
        </property>
        <property name="dealerCompanyId">
            <column name="dealer_company_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="onlyShip">
            <column name="only_ship" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>