<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


    <class name="dao.CashDetailEntity" table="cash_detail" schema="public" catalog="charge_db">
        <id name="cashDetailId">
            <column name="cash_detail_id" sql-type="serial" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="cashMasterId">
            <column name="cash_master_id" sql-type="int4"/>
        </property>
        <property name="calYm">
            <column name="cal_ym" sql-type="varchar" length="6"/>
        </property>
        <property name="companyId">
            <column name="company_id" sql-type="int4"/>
        </property>
        <property name="cashType">
            <column name="cash_type" sql-type="int4"/>
        </property>
        <property name="billType">
            <column name="bill_type" sql-type="int4"/>
        </property>
        <property name="orgPrice">
            <column name="org_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="diffPrice">
            <column name="diff_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="diffPriceNote">
            <column name="diff_price_note" sql-type="varchar" length="100"/>
        </property>
        <property name="noTaxInclusivePrice">
            <column name="no_tax_inclusive_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="taxType">
            <column name="tax_type" sql-type="bpchar" length="1"/>
        </property>
        <property name="taxRate">
            <column name="tax_rate" sql-type="float4" precision="8" scale="8"/>
        </property>
        <property name="taxPrice">
            <column name="tax_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="taxInclusivePrice">
            <column name="tax_inclusive_price" sql-type="numeric" precision="32" scale="9"/>
        </property>
        <property name="creatorId">
            <column name="creator_id" sql-type="int4"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="timestamp"/>
        </property>
        <property name="modifierId">
            <column name="modifier_id" sql-type="int4"/>
        </property>
        <property name="modifyDate">
            <column name="modify_date" sql-type="timestamp"/>
        </property>
        <property name="billId">
            <column name="bill_id" sql-type="int4"/>
        </property>
        <property name="packageId">
            <column name="package_id" sql-type="int4"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int4"/>
        </property>
        <property name="outYm">
            <column name="out_ym" sql-type="varchar" length="6"/>
        </property>
        <property name="commissionLogId">
            <column name="commission_log_id" sql-type="int4"/>
        </property>
        <property name="commissionAmount">
            <column name="commission_amount" sql-type="numeric" precision="53" scale="4"/>
        </property>
        <property name="isFirst">
            <column name="is_first" sql-type="varchar" length="1"/>
        </property>
    </class>
</hibernate-mapping>